{
    "manifest_version": 2,
    "background": {
        "scripts": [
            "assets/js/background.js"
        ],
        "persistent": false
    },
    "name": "YouTubeMeow",
    "version": "0.0.100",
    "description": "Adds a YouTube button on the Google search page allowing you to easily transfer your query to YouTube. For when you need to search YouTube right Meow!",
    "icons": {
        "16": "assets/images/icon-16.png",
        "32": "assets/images/icon-32.png",
        "48": "assets/images/icon-48.png",
        "96": "assets/images/icon-96.png",
        "128": "assets/images/icon-128.png"
    },
    "author": "Kanon Lo",
    "permissions": [
        "contextMenus"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://www.google.com/search*"
            ],
            "js": [
                "assets/js/content_scripts.js"
            ]
        }
    ]
}